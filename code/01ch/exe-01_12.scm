(define (pascal x y)
  (cond ((or (= x 0) (= y 0)) 1)
        (else (+ (pascal x (- y 1)) (pascal (- x 1) y)))))


(define (test x y)
  (write x)
  (write " ")
  (write y)
  (write "=")
  (write (pascal x y))
  (newline)
)

(test 0 1)
(test 1 1)
(test 2 1)
(test 3 1)
(test 4 1)
(test 5 1)
(test 2 0)
(test 2 5)
(test 2 10)
(test 8 9)
(test 0 99)
